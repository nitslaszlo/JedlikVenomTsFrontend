{
  "name": "jedlik-venom-ts-frontend",
  "description": "Frontend with Vue.js, BootstrapVue, Vuex, Node.js, Axios, and TypeScript",
  "version": "0.1.0",
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --fix",
    "kill": "taskkill /im node.exe /F"
  },
  "author": "Nits László",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/nitslaszlo/JedlikVenomTsFrontend"
  },
  "devDependencies": {
    "@types/node": "^12.7.12",
    "@types/axios": "0.14.0",
    "@vue/cli-plugin-eslint": "^4.0.5",
    "@vue/cli-plugin-router": "^4.0.5",
    "@vue/cli-plugin-typescript": "^4.0.5",
    "@vue/cli-plugin-vuex": "^4.0.5",
    "@vue/cli-service": "^4.0.5",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "typescript": "~3.5.3",
    "vue-template-compiler": "^2.6.10"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "jquery": "3.4.1",
    "bootstrap-vue": "^2.0.4",
    "portal-vue": "^2.1.6",
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.1",
    "vuex-module-decorators": "^0.10.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard",
      "@vue/typescript"
    ],
    "rules": {
      "semi": [
        "error",
        "always"
      ],
      "quotes": [
        "error",
        "double"
      ],
      "space-before-function-paren": [
        "error",
        "never"
      ]
    },
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
